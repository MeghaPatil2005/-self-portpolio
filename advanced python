        "class Parent:\n",
        "    def show_parent(self):\n",
        "        print(\"This is Parent class\")\n",
        "\n",
        "class Child(Parent):\n",
        "    def show_child(self):\n",
        "        print(\"This is Child class\")\n",
        "\n",
        "c = Child()\n",
        "c.show_parent()\n",
        "c.show_child()\n"
